generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int      @id @default(autoincrement())
  nome          String
  email         String   @unique
  senha         String
  cargo         String
  status        Boolean  @default(true)
  dataAdmissao  DateTime @default(now())
}
